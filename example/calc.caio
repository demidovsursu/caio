%{
#include <stdio.h>
%}
%option bnf interactive
%operator <yfx> '+' '-'
%operator <yfx> '*' '/'
%operator <fy> '-'
%%
\d+ { sscanf(yytext,"%d",&$$); return ?number?; }
. ;
%%
program: 
  | program expr '\n' { printf("%d\n",$2); }
  ;
expr: 
    expr '+' expr { $$ = $1+$3; }
  | expr '-' expr { $$ = $1-$3; }
  | expr '*' expr { $$ = $1*$3; }
  | expr '/' expr { $$ = $1/$3; }
  | '-' expr  { $$ = -$2; }
  | '(' expr ')' 
  | ?number?
  ;
%%
